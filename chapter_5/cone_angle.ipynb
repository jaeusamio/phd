   "source": [
    "import os\n",
    "from morfeus import ConeAngle, read_xyz\n",
    "\n",
    "for file in os.listdir('xyz'):\n",
    "    elements, coordinates = read_xyz(f\"xyz/{file}\")\n",
    "    cone_angle = ConeAngle(elements, coordinates, 1)\n",
    "    print(file)\n",
    "    cone_angle.print_report()\n",
    "    print(\"\")"
